<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="foggyline_catalog_homepage" path="/">
        <default key="_controller">FoggylineCatalogBundle:Default:index</default>
    </route>

    <route id="foggyline_catalog_fetch_catalog" path="/fetch">
        <default key="_controller">FoggylineCatalogBundle:Default:fetch</default>
    </route>
    <!-- Categorry -->
    <route id="foggyline_catalog_update_catalog_category" path="/edit/">
        <default key="_controller">FoggylineCatalogBundle:AjaxCategory:edit</default>
    </route>
    <route id="foggyline_catalog_update_catalog_categoryGet" path="/{id}/edit">
        <default key="_controller">FoggylineCatalogBundle:AjaxCategory:edit</default>
    </route>

    <route id="foggyline_catalog_add_catalog_category" path="/add">
        <default key="_controller">FoggylineCatalogBundle:AjaxCategory:add</default>
    </route>

    <route id="foggyline_catalog_fetch_catalog_category" path="/fetch">
        <default key="_controller">FoggylineCatalogBundle:AjaxCategory:fetch</default>
    </route>
    <!-- //Categorry -->
    <!-- Sub Cathegory -->
   
    <!-- // Sub Cathegory -->
</routes>
