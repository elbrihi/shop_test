<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="foggyline_user_homepage" path="/">
        <default key="_controller">FoggylineUserBundle:Default:index</default>
    </route>

    <route id="foggyline_user_fetch_user" path="/fetch/">
        <default key="_controller">FoggylineUserBundle:Default:fetch</default>
    </route>

    <route id="foggyline_user_edit_user" path="/edit/">
        <default key="_controller">FoggylineUserBundle:Default:edit</default>
    </route>
    <route id="foggyline_user_add_user" path="/add/">
        <default key="_controller">FoggylineUserBundle:Default:add</default>
    </route>

    <route id="foggyline_user_delete_user" path="/delete/">
        <default key="_controller">FoggylineUserBundle:Default:delete</default>
    </route>
    
    
</routes>
